<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">
 <managed-bean>
  <managed-bean-name>QuestionBean</managed-bean-name>
  <managed-bean-class>bean.CreateQuestionsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>

 <managed-bean>
  <managed-bean-name>showQuestionsBean</managed-bean-name>
  <managed-bean-class>bean.ShowQuestionsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>

 <managed-bean>
  <managed-bean-name>facadeBean</managed-bean-name>
  <managed-bean-class>bean.FacadeBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>Main.xhtml</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>create</from-outcome>
   <to-view-id>/CreateQuestion.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>Main.xhtml</display-name>
  <from-view-id>/Main.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>show</from-outcome>
   <to-view-id>/ShowQuestions.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
